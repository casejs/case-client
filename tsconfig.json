  "compilerOptions": {
    "outDir": "./dist/",
    "lib": ["dom", "esnext"],
    "target": "es2016",
    "module": "ESNext",
    "moduleResolution": "node",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
